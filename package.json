{
  "name": "@JarCz/cs3_jlab_test_ext",
  "version": "0.1.0",
  "description": "CS3APIs Jupyterlab contents connector",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "CS3APIs",
    "CS3"
  ],
  "homepage": "https://github.com/JarCz/cs3_jlab_test_ext",
  "bugs": {
    "url": "https://github.com/JarCz/cs3_jlab_test_ext/issues"
  },
  "license": "Apache-2.0",
  "author": "cs3_jlab_test_ext",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/JarCz/cs3_jlab_test_ext.git"
  },
  "scripts": {
    "build": "jlpm run build:lib",
    "build:labextension": "cd cs3api_test_ext && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:all": "jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf cs3api_test_ext/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "3.0.0-alpha.2",
    "@jupyterlab/coreutils": "^4.0.0",
    "@jupyterlab/filebrowser": "^2.1.2",
    "@jupyterlab/launcher": "^2.1.1",
    "@jupyterlab/mainmenu": "^2.2.0",
    "@jupyterlab/services": "^5.1.0",
    "@jupyterlab/settingregistry": "^2.1.0",
    "@lumino/widgets": "^1.13.4",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mkdirp": "^1.0.3",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "cs3_jlab_test_ext"
        }
      }
    },
    "extension": true
  }
}
